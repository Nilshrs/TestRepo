
<!--suppress JSNonStrictModeUsed -->
<main>

    <div class="container">

    <div class="row mx-1">
        <a class =" text-white text-decoration-none" href="/watchlist"> ‚ùÆ  </a>
    </div>
        <div class="row">
            <div class="table-responsive">
                <table  id="tb" class = "table table-striped table-dark">
                    <thead>
                    <tr class="info">
                        <th>Token</th>
                        <th>Currency</th>
                        <th>Price</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% if (tokens) { %>
                        <% tokens.forEach((token) => { %>
                            <tr>
                                <td>
                                    <%= token.name %>
                                </td>
                                <td>
                                    <%= token.currency %>
                                </td>
                                <td>
                                    <%= token.price %>
                                </td>
                            </tr>
                    <% }); }%>
                    </tbody>
                </table>
            </div>
        </div>
    </div>







            <!--TODO backwards button to go to allToken view-->


</main>


